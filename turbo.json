{
  "$schema": "https://turborepo.org/schema.json",
  "envMode": "loose",
  "tasks": {
    "topo": {
      "dependsOn": [
        "^topo"
      ]
    },
    "build": {
      "dependsOn": [
        "^build"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": false,
      "dependsOn": [
        "^build",
        "^topo"
      ]
    },
    "format": {},
    "lint": {
      "dependsOn": [
        "^topo",
        "^build"
      ]
    },
    "typecheck": {
      "dependsOn": [
        "^topo",
        "^build"
      ]
    },
    "test": {
      "dependsOn": [
        "^topo",
        "^build"
      ]
    }
  }
}